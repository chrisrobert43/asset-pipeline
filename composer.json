{
    "name": "codesleeve/asset-pipeline",
    "description": "A very simple and easy to use asset pipeline for laravel 4.",
    "authors": [
        {
            "name": "Kelt Dockins",
            "email": "kelt@dockins.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.0.x",
        "kriswallsmith/assetic": "1.2.*@dev",
        "leafo/lessphp": "0.3.*@dev",
        "natxet/CssMin": "dev-master",
        "coffeescript/coffeescript": "dev-master",
        "nitra/php-min": "dev-master"
    },
    "require-dev": {
        "mikey179/vfsStream": "1.3.*@dev",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/models"
        ],
        "psr-0": {
            "Codesleeve\\AssetPipeline": "src/"
        }
    },
    "minimum-stability": "dev"
}